        "xp": {
            "name": "xp",
            "description": "Replies with your current XP!",
            "current_xp": "Your current XP: {serverXp}",
            "usage": "/xp"
        },
        "xpuser": {
            "name": "xp",
            "description": "Replies with your current XP!",
            "usage": "/xp [action]",
            "subcommands": {
                "rank": {
                    "name": "rank",
                    "description": "Shows the TOP 10 XP on the server",
                    "response": {
                        "title": "Top 10",
                        "not_found": "XP data were not found on this server."
                    }
                }
            }
        },
        "xpadmin": {
            "name": "xp",
            "description": "administrate users XP!",
            "usage": "/xp [action]",
            "subcommands": {
                "add": {
                    "name": "add",
                    "description": "Add XP to a user",
                    "options": {
                        "user": {
                            "name": "user",
                            "description": "User that will receive XP"
                        },
                        "amount": {
                            "name": "amount",
                            "description": "Given XP Amount"
                        }
                    },
                    "response": {
                        "title": "Add XP",
                        "added": "{amount}xp Added to {user}",
                        "error": "An error ocurred!"
                    }
                },
                "remove": {
                    "name": "remove",
                    "description": "Remove XP to a user",
                    "options": {
                        "user": {
                            "name": "user",
                            "description": "User that will loose XP"
                        },
                        "amount": {
                            "name": "amount",
                            "description": "Removed XP Amount"
                        }
                    },
                    "response": {
                        "title": "Remove XP",
                        "removed": "{amount}xp Removed to {user}",
                        "error": "An error ocurred!"
                    }
                },
                "reset": {
                    "name": "reset",
                    "description": "Reset all users XP",
                    "response": {
                        "title": "Reset XP",
                        "reseted": "XP reseted on the server!",
                        "error": "An error ocurred!"
                    }
                }
            }
        },
        "disablexp": {
            "name": "disablexp",
            "description": "Disable a channel from receiving XP",
            "response_title": "Disable XP",
            "channel_already_disabled": "This channel already has XP disabled!",
            "xp_disabled": "XP disabled on channel <#{channel_id}>",
            "usage": "/disablexp <channel>",
            "options": {
                "channel": {
                    "name": "channel",
                    "description": "Channel that will disable XP"
                }
            }
        },
        "enablexp": {
            "name": "enablexp",
            "description": "Enable a channel from receiving XP",
            "response_title": "Enable XP",
            "channel_already_enabled": "This channel already has XP enabled!",
            "xp_enabled": "XP enabled on channel <#{channel_id}>",
            "usage": "/enablexp <channel>",
            "options": {
                "channel": {
                    "name": "channel",
                    "description": "Channel that will enable XP"
                }
            }
        },
        "setchannel": {
            "name": "setchannel",
            "description": "Setup a channel for automatic messages",
            "response_title": "Set Channel",
            "response_body": "{channelType} changed successfully!",
            "usage": "/setchannel <channel type> <channel>",
            "options": {
                "channeltype": {
                    "name": "channeltype",
                    "description": "Type of the channel to be set",
                    "choices": {
                        "welcome_channel": {
                            "name": "Welcome Channel"
                        },
                        "birthday_channel": {
                            "name": "Birthday Notification Channel"
                        },
                        "voice_activity_log_channel": {
                            "name": "Call logs Channel"
                        }
                    }
                },
                "channel": {
                    "name": "channel",
                    "description": "Channel to be set"
                }
            }
        },
