    "commands": {
        "help": {
            "name": "help",
            "description": "Replies with Command Help!",
            "usage": "/help",
            "template": "Command Description: {commandDescription}\nUsage: {commandUsage}"
        },
        "ping": {
            "name": "ping",
            "description": "Replies with Pong!",
            "usage": "/ping"
