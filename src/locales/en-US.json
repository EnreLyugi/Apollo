            "response": {
                "title": "Color Role",
                "role_already_exists": "This role is already set as a Color role.",
                "role_setup": "Role **{role}** Added for color **{name}**."
            }
        },
        "removecolorrole": {
            "name": "removecolorrole",
            "description": "Remove color roles from the shop.",
            "options": {
                "role": {
                    "name": "role",
                    "description": "Role that will be removed."
                }
